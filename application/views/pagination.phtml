{%if pages > 1%}    <ul class="pagination" style="margin-top:10px">        {%if page > 1%}            <li><a href="/search?kw={{kw}}&od={{orderby}}&page={{page-1}}">Prev</a></li>            <!--li><a href="/search/{{kw}}_{{orderby}}_{{page-1}}.html">Prev</a></li-->        {%endif%}        {%for i in range(1, 6)%}            {%if page-i > 0%}                <li><a href="/search?kw={{kw}}&od={{orderby}}&page={{page-i}}">{{page-i}}</a></li>                <!--li><a href="/search/{{kw}}_{{orderby}}_{{page-i}}.html">{{page-i}}</a></li-->            {%endif%}        {%endfor%}        <li class="disabled"><a href="/search/?kw={{kw}}&od={{orderby}}&page={{page}}">{{page}}</a></li>        <!--li class="disabled"><a href="/search/{{kw}}_{{orderby}}_{{page}}.html">{{page}}</a></li-->        {%for i in range(1, 6)%}            {%if page+i <= pages%}                <li><a href="/search?kw={{kw}}&od={{orderby}}&page={{page+i}}">{{page+i}}</a></li>                <!--li><a href="/search/{{kw}}_{{orderby}}_{{page+i}}.html">{{page+i}}</a></li-->            {%endif%}        {%endfor%}        {%if page < pages%}            <li><a href="/search?kw={{kw}}&od={{orderby}}&page={{page+1}}">Next</a></li>            <!--li><a href="/search/{{kw}}_{{orderby}}_{{page+1}}.html">Next</a></li-->        {%endif%}      </ul>{%endif%}